{
  "kind": "ds",
  "id": "ds.claude_control_profile_v0",
  "version": "0.1.0",
  "title": "Claude Control Profile v0 (strict)",
  "mode": "report_only",
  "anthropic": {
    "permissions": {
      "allow": [
        "fs.read",
        "fs.write",
        "process.spawn"
      ],
      "deny": [
        "network.raw"
      ],
      "behavior": {
        "on_conflict": "deny_wins",
        "on_unknown": "report_only"
      }
    },
    "mcp": {
      "servers": {},
      "env_expansion_policy": {
        "require_all_vars": true,
        "allow_defaults_syntax": false,
        "on_missing_var": "report_only"
      }
    },
    "plugins": {
      "enable": true,
      "allowed_plugin_ids": [
        "local-tools"
      ],
      "denied_plugin_ids": [],
      "behavior": {
        "on_unknown": "report_only"
      }
    },
    "claude_md": {
      "inject_control_entry": true,
      "marker": "<!-- MOVA_CONTROL_ENTRY_V0 -->"
    }
  },
  "mova": {
    "overlay": {
      "emit": true
    },
    "observability": {
      "emit_version_anchor": true,
      "emit_manifest": true,
      "emit_policy_report": true,
      "emit_apply_report": true,
      "emit_quality_report": true,
      "artifact_root": "mova/claude_import/v0"
    },
    "export": {
      "emit_zip": true,
      "zip_name": "export.zip",
      "emit_export_manifest": true
    }
  },
  "apply": {
    "default_apply_mode": "preview",
    "default_target": "project_only",
    "backup_before_write": true
  }
}
